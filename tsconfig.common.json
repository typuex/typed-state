{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declarationDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": [
      "DOM",
      "ES2020"
    ],
    "strictNullChecks": true,
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "declaration": true,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noEmitOnError": true,
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "stripInternal": true
  }
}
